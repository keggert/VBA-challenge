Sub multiple_year_stock_data():

        Dim ws As Worksheet
        Dim ticker As String
        Dim LastRow As Long
        Dim year_open As Double
        Dim year_close As Double
        Dim totaltickervolume As Double
        Dim yearly_change As Double
        Dim percent_change As Double
        Dim summary_table_row As Long
        Dim average_change As Double
        Dim variables
        Dim i As Long
        Dim j As Integer
        Dim start As Long
        ' Dim Counters one for each row in the worksheet, one for a block, and another for ticker
        
        'Determine the last row
        LastRow = Cells(Rows.Count, 1).End(xlUp).Row
        
        summary_table_row = 2
        year_open = 0
        year_close = 0
        yearly_change = 0
        percent_change = 0
        totaltickervolume = 0
        start = 2
        j = 0
        
        Max = 0
        Min = 0
     
        'Create column Headers
        Range("I1").Value = "Ticker"
        Range("J1").Value = "Yearly Change"
        Range("K1").Value = "Percent Change"
        Range("L1").Value = "Total Stock Volume"
        Range("P1").Value = "Ticker"
        Range("Q1").Value = "Value"
        Range("O2").Value = "Greatest % Increase"
        Range("O3").Value = "Greatest % Decrease"
        Range("O4").Value = "Greatest Total Volume"
        
        summary_table_row = 2
        year_open = 0
        year_close = 0
        yearly_change = 0
        percent_change = 0
        volume = 0
        start = 2
        j = 0
        
        Max = 0
        Min = 0

        
        'Loop to search through each row
        For i = 2 To LastRow
            If Cells(i + 1, 1).Value <> Cells(i, 1).Value Then
                ticker = Cells(i, 1).Value
                Cells(summary_table_row, 9).Value = Cells(i, 1).Value
                
                volume = volume + Cells(i, 7).Value
                Cells(summary_table_row, 12).Value = volume
                
                'Calculate Change
                year_open = Cells(i, 3).Value
                year_close = Cells(i, 6).Value
                yearly_change = (Cells(i, 6).Value - Cells(start, 3))
                Cells(summary_table_row, 10).Value = yearly_change
                
                percent_change = Round((yearly_change / Cells(start, 3) * 100), 2)
                percent_change = Cells(summary_table_row, 11).Value
                
                If year_open = 0 And year_close = 0 Then
                    Cells(summary_table_row, 11).Value = 0
                Else
                    Cells(summary_table_row, 11).Value = yearly_change / year_close
                End If
                
            'Reset variables
            summary_table_row = summary_table_row + 1
            
            percent_change = 0
            yearly_change = 0
            year_close = 0
            volume = 0
            year_open = Cells(i + 1, 3).Value
            
            If Cells(i, 10).Value > 0 Then
                Cells(summary_table_row, 10).Interior.ColorIndex = 4
            Else
                Cells(summary_table_row, 10).Interior.ColorIndex = 3
                
            End If
            
        'Fixing row year 2 start
        
            Else
                volume = volume + Cells(i, 7).Value
            
        End If
            
        Next i
             

End Sub
